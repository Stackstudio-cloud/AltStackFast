{
  "version": 2,
  "buildCommand": "turbo run build --filter=@altstackfast/api",
  "outputDirectory": "packages/api/dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "packages/api/dist/server.js"
    }
  ],
  "builds": [
    {
      "src": "packages/api/dist/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["packages/api/dist/**"]
      }
    }
  ]
}