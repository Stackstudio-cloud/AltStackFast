{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://altstackfast.com/schemas/toolProfile.schema.json",
  "title": "Tool Profile",
  "description": "A profile for an AI development tool in the AltStackFast registry.",
  "type": "object",
  "properties": {
    "tool_id": { "type": "string" },
    "name": { "type": "string" },
    "description": { "type": "string" },
    "category": { "type": "array", "items": { "type": "string" } },
    "notable_strengths": { "type": "array", "items": { "type": "string" } },
    "known_limitations": { "type": "array", "items": { "type": "string" } },
    "output_types": { "type": "array", "items": { "type": "string" } },
    "integrations": { "type": "array", "items": { "type": "string" } },
    "license": { "type": ["string", "null"] },
    "maturity_score": { "type": ["number", "null"], "minimum": 0, "maximum": 1 },
    "last_updated": { "type": "string", "format": "date-time" },
    "schema_version": { "type": "string" },
    "requires_review": { "type": "boolean" }
  },
  "required": [
    "tool_id",
    "name",
    "description",
    "category",
    "last_updated",
    "schema_version"
  ]
} 